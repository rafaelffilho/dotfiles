# Aliases
alias md='mkdir -p'
alias v='vim'
alias javar='java -jar'
alias ping=' ping -c 5'
alias 'cd..'='cd ..'
alias shr='source ~/.zshrc'
alias she='vim ~/.zshrc'
alias ls='ls --color=auto'
alias :3='echo'
alias gc='git commit'
alias ga='git add .'
alias gs='git status'
alias gi='git init .'
alias yolo='git commit -m "$(curl -s whatthecommit.com/index.txt)"'
alias bye='shutdown now'
alias fkbye='halt'
alias cya='reboot'
alias please='sudo'
alias dn='date +"%Y%m%d %H%M %Z"'
